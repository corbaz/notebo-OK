{
    "name": "notebo-ok",
    "version": "2.0.0",
  "scripts": {
    "build": "svite build -ts",
    "validate": "svelte-check && tsc --noEmit",
    "dev": "svite -ts -o",
    "update": "ncu -u && pnpm install",
    "ncu": "ncu",
    "pnpm-install": "pnpm install",
    "optimizer": "vite optimize --force",
    "test": "jest",
    "notebo-ok": "npm run build && cd dist && surge --domain notebo-ok.surge.sh",
    "corbaz": "npm run build && cd dist && surge --domain corbaz.surge.sh"
  },
  "dependencies": {
    "register-service-worker": "^1.7.1",
    "svelte-awesome": "^2.3.0"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^1.0.10",
    "postcss": "^8.1.4",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "svelte-check": "^1.1.3",
    "svelte-preprocess": "4.5.2",
    "svelte": "3.29.4",
    "svelte-hmr": "0.11.3",
    "svite": "^0.8.0",
    "tailwindcss": "^1.9.6",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  }
}