{
  "name": "notebo-ok",
  "version": "2.0.0",
  "scripts": {
    "build": "svite build -ts --assetsDir . --sourcemap true && gz.bat v.20.11 && cd dist && surge --domain notebo-ok.surge.sh && cd .. && pause 'arreglar index.html y css' && deploy",
    "deploy": "gh-pages -d dist -b master -e docs",
    "validate": "svelte-check && tsc --noEmit",
    "dev": "svite -ts -o",
    "remote": "git remote add origin https://github.com/corbaz/notebo-OK.git",
    "add": "git add .",
    "commit": "git commit -m commit",
    "push": "git push origin master",
    "update": "ncu -u && pnpm install",
    "ncu": "ncu",
    "pnpm-install": "pnpm install",
    "optimizer": "vite optimize --force",
    "test": "jest",
    "notebo-ok": "npm run build && cd dist && surge --domain notebo-ok.surge.sh",
    "corbaz": "npm run build && cd dist && surge --domain corbaz.surge.sh"
  },
  "dependencies": {
    "register-service-worker": "^1.7.1",
    "svelte-awesome": "^2.3.0",
    "timeUtils": "2.0.0"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^1.0.10",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.4",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "serve": "^11.3.2",
    "svelte": "3.29.4",
    "svelte-check": "^1.1.6",
    "svelte-hmr": "0.11.5",
    "svelte-preprocess": "4.5.2",
    "svite": "^0.8.0",
    "tailwindcss": "^1.9.6",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  }
}